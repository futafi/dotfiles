snippet figure
alias   \begin{figure} \figure
	\begin{figure}[${1}]
		\centering
		\includegraphics[${2:width=${3\}}]{${4}}
		\caption{${5}}
		\label{fig:${6}}
	\end{figure}
  ${0}
  
snippet table
alias   \begin{table} \table
	\begin{table}[${1}]
		\centering
		\caption{${2}}
		\label{tab:${3}}
		\begin{tabular}{${4}}
			${5}
		\end{tabular}
	\end{table}
  ${0}
    
snippet \bef
alias   bef
	\begin{figure}[${1:H}]
        \centering
        \includegraphics[${2:width=${3\}}]{${4}}
        \caption{${5:#:caption}}
        \label{fig:${6}}
	\end{figure}
  ${0}
  
snippet \bet
alias   bet
	\begin{table}[${1:H}]
        \centering
        \caption{${2}}
        \label{tab:${3}}
        \includegraphics[${4:width=${5\}}]{${6}}
	\end{table}
  ${0}
  
